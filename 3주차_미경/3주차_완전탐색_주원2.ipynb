   "source": [
    "## Q3. 지뢰찾기 (완전탐색2)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "N = int(input())\n",
    "Q = [input() for i in range(N)]\n",
    "\n",
    "answer = 0\n",
    "for i in range(1, N-1):\n",
    "    for j in range(1, N-1):\n",
    "        if '0' not in Q[i-1][j-1:j+2] + Q[i][j-1:j+2] + Q[i+1][j-1:j+2]:\n",
    "            answer += 1\n",
    "print(answer)"
   ]
  }
